{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "module": true,
    "define": true
  },
  "plugins": [
    "react"
  ],
  "rules":{
    "indent": ["error", 2],
    "no-console": 0,
    "semi": 0,
    "camelcase": 2,
    "max-params": [2, 5],
    "max-depth": [2, 2],
    "complexity": [2, 6],
    "max-len": [2, 160, 4],
    "new-cap": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-uses-react": 1
  }
}
